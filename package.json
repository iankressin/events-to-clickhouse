{
  "name": "events-to-table",
  "version": "0.0.6",
  "scripts": {
    "build": "tsup",
    "start": "node bin/index.js",
    "watch": "tsc --watch",
    "dev:contract": "cross-env ts-node-dev --respawn --transpile-only src/index.ts generate --from address --etherscan $ETHERSCAN_API --contract 0x947f0054faed3481ff4e76ca35f12fbe36cc665b --output ./custom.sql --case preserve",
    "dev:abi": "cross-env ts-node-dev --respawn --transpile-only src/index.ts generate --from abi --abi ./abi.json",
    "lint:fix": "biome check . --write",
    "prepublishOnly": "pnpm build"
  },
  "bin": {
    "ets": "bin/index.js"
  },
  "exports": {
    ".": "./bin/index.js"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.3",
    "@types/node": "^24.1.0",
    "cross-env": "^10.0.0",
    "ts-node-dev": "^2.0.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "commander": "^14.0.0",
    "effect": "^3.17.4",
    "inquirer": "^12.8.2",
    "zod": "^4.0.10"
  },
  "packageManager": "pnpm@10.12.1+sha512.f0dda8580f0ee9481c5c79a1d927b9164f2c478e90992ad268bbb2465a736984391d6333d2c327913578b2804af33474ca554ba29c04a8b13060a717675ae3ac"
}
